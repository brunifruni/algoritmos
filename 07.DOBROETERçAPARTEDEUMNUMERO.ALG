algoritmo "07.dobroeterçapartedeumnumero"
var
    n, dobro: inteiro
    terca: real
inicio
       escreval ("Digite um número: ")
       leia (n)
       dobro <- n * n
       terca <- n /3
       escreval ("O dobro de", n," é", dobro)
       escreval ("A terça parte de", n, " é", terca)
fimalgoritmo