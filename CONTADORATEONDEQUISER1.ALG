algoritmo "contadorateondequiser"
var
    contador,n :inteiro
inicio
     escreva ("Digite um numero: ")
     leia (n)
     contador <- 0
     enquanto (contador <=n) faça
     escreval (contador)
     contador <- contador + 1
     fimenquanto
     escreval ("Terminei de contar!")

fimalgoritmo