algoritmo "atividade 8"
var
   d1: real

inicio
      escreva (" Digite uma distância em metros: ")
      leia (d1)
      escreval ("A distância de " ,d1, "metros" ," corresponde a: ")
      escreval (d1*10, "dm")
      escreval (d1*100, "cm")
      escreval (d1*1000, "mm")
      escreval (d1/10, "dam")
      escreval ( d1/100, "hm")
      escreval (d1/1000, "km")


fimalgoritmo
